; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_angle_interpolation_type_conflicting=false

[application]

config/name="Pixel-Crawler"
run/main_scene="res://Entities/Zones/castle.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")

[autoload]

InteractionManager="*res://Common/Interaction/InteractionManager.tscn"
DamageNumbers="*res://Common/damage_numbers.gd"
StatsManager="*res://Common/Stats/stats_manager.gd"
Math="*res://Common/math.gd"
AttackCooldowns="*res://Entities/Weapons/attack_cooldowns.gd"
WaveStatsOverview="*res://Common/wave_stats_overview/wave_stats_overview.gd"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/always_on_top=true
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[file_customization]

folder_colors={
"res://Common/": "green",
"res://Entities/": "red",
"res://Items and Drops/": "blue",
"res://UI/": "yellow",
"res://assets/": "purple"
}

